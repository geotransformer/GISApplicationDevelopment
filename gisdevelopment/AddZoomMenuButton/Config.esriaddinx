      <Commands>
        <Button id="Penn_AddZoomMenuButton_ZoomButton" class="ZoomButton" message="Add-in command generated by Visual Studio project wizard." caption="Zoom Button" tip="Add-in command tooltip." category="Add-In Controls" image="Images\ZoomButton.png" />
      </Commands>
    </ArcMap>
  </AddIn>
</ESRI.Configuration>