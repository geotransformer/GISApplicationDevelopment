  <Description>Add a zoom menu button at the ArcMap toolbox</Description>
  <Version>1.0</Version>
  <Image>Images\AddZoomMenuButton.png</Image>
  <Author>Vincent</Author>
  <Company>Penn</Company>
  <Date>2017/2/17</Date>
  <Targets>
    <Target name="Desktop" version="10.3" />
  </Targets>
  <AddIn language="CLR4.5" library="AddZoomMenuButton.dll" namespace="AddZoomMenuButton">
